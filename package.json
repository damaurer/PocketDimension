		"typescript": "^5.6.2",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.0"
	},
	"type": "module",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"dockerode": "^4.0.2",
		"jsonwebtoken": "^9.0.2",
		"sqlite3": "^5.1.7"
